{
    "name": "shooglypeg/emailvalidator",
    "description": "email validator",
    "authors": [
        {
            "name": "Peter Somerville",
            "email": "peterwsomerville@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "egulias/email-validator": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ShooglyPeg\\EmailValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
